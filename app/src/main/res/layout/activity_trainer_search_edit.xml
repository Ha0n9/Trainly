<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TrainerSearchEditActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/trainerSearchTitle"
        android:textSize="32sp"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="36dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvClientExerciseInfoSearchEdit"
        android:layout_width="406dp"
        android:layout_height="200dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="189dp" />

    <Button
        android:id="@+id/btnBackToMainSearchEdit"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:text="@string/btnBackToMainAssign"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="500dp" />

    <Spinner
        android:id="@+id/spnChooseClientSearchEdit"
        android:layout_width="279dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="137dp" />

    <Button
        android:id="@+id/btnEditSearchEdit"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:text="@string/btnEditSearchEdit"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="441dp" />
</androidx.constraintlayout.widget.ConstraintLayout>